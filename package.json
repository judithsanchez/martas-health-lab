{
    "name": "martas-health-lab-monorepo",
    "private": true,
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "dev": "pnpm --filter web dev",
        "build": "pnpm --filter web build",
        "db:push": "pnpm --filter db db:push",
